<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:orientation="vertical"
	android:gravity="top"
	android:background="#FF61CBFF">
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="#FF3DBFFF"
		android:padding="8dp"
		android:gravity="center_horizontal|right">
	<LinearLayout
		android:id="@+id/mwWinBoxLinear"
		android:layout_height="wrap_content"
		android:layout_width="wrap_content"
		android:padding="8dp"
		android:layout_margin="8dp"
		android:gravity="center_vertical|center_horizontal"
		android:background="@drawable/mw_anybox_layout">
		<ImageView
			android:id="@+id/mwWinBox"
			android:src="@drawable/magicworld_icon_additional_ability"
			android:layout_width="28dp"
			android:layout_height="28dp"/>
		<TextView
			android:id="@+id/mwWinBoxCupCount"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textColor="#000000"
			android:textSize="14sp"
			android:padding="3dp"
			android:text="100"/>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/mwDataBoxLinear"
		android:layout_height="wrap_content"
		android:layout_width="wrap_content"
		android:padding="8dp"
		android:layout_margin="8dp"
		android:gravity="center_vertical|center_horizontal"
		android:background="@drawable/mw_anybox_layout">
		<ImageView
			android:id="@+id/mwDataBoxLevel"
			android:layout_width="28dp"
			android:layout_height="28dp"
			android:src="@drawable/magicworld_icon_level"/>
		<TextView
			android:id="@+id/mwDataBoxLevelCount"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textColor="#000000"
			android:textSize="14sp"
		    android:padding="3dp"
			android:text="100"/>
		<ImageView
			android:id="@+id/mwDataBoxCoin"
			android:layout_width="28dp"
			android:layout_height="28dp"
			android:src="@drawable/magicworld_icon_coin"/>
		<TextView
			android:id="@+id/mwDataBoxCoinCount"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textColor="#000000"
			android:textSize="14sp"
			android:padding="3dp"
			android:text="100"/>
	</LinearLayout>
	</LinearLayout>
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical"
		android:gravity="center_horizontal|center_vertical"
		android:layout_weight="1">
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:gravity="center_horizontal|center_vertical">
		<ImageView
            android:clickable="true"
            android:foreground="?android:attr/selectableItemBackgroundBorderless"
			android:id="@+id/mwBlockTab1"
			android:layout_width="55dp"
			android:layout_height="55dp"
			android:src="@drawable/magicworld_block_grass"
			android:background="@drawable/magicworld_block_grass"/>
		<ImageView
            android:clickable="true"
            android:foreground="?android:attr/selectableItemBackgroundBorderless"
			android:id="@+id/mwBlockTab2"
			android:layout_width="55dp"
			android:layout_height="55dp"
			android:src="@drawable/magicworld_block_grass"
			android:background="@drawable/magicworld_block_grass"/>
		<ImageView
            android:clickable="true"
            android:foreground="?android:attr/selectableItemBackgroundBorderless"
			android:id="@+id/mwBlockTab3"
			android:layout_width="55dp"
			android:layout_height="55dp"
			android:src="@drawable/magicworld_block_grass"
			android:background="@drawable/magicworld_block_grass"/>
		<ImageView
            android:clickable="true"
            android:foreground="?android:attr/selectableItemBackgroundBorderless"
			android:id="@+id/mwBlockTab4"
			android:layout_width="55dp"
			android:layout_height="55dp"
			android:src="@drawable/magicworld_block_grass"
			android:background="@drawable/magicworld_block_grass"/>
		<ImageView
            android:clickable="true"
            android:foreground="?android:attr/selectableItemBackgroundBorderless"
			android:id="@+id/mwBlockTab5"
			android:layout_width="55dp"
			android:layout_height="55dp"
			android:src="@drawable/magicworld_block_grass"
			android:background="@drawable/magicworld_block_grass"/>
	</LinearLayout>
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:gravity="center_horizontal|center_vertical">
			<ImageView
				android:clickable="true"
				android:foreground="?android:attr/selectableItemBackgroundBorderless"
				android:id="@+id/mwBlockTab6"
				android:layout_width="55dp"
				android:layout_height="55dp"
				android:src="@drawable/magicworld_block_grass"
				android:background="@drawable/magicworld_block_grass"/>
			<ImageView
				android:clickable="true"
				android:foreground="?android:attr/selectableItemBackgroundBorderless"
				android:id="@+id/mwBlockTab7"
				android:layout_width="55dp"
				android:layout_height="55dp"
				android:src="@drawable/magicworld_block_grass"
				android:background="@drawable/magicworld_block_grass"/>
			<ImageView
				android:clickable="true"
				android:foreground="?android:attr/selectableItemBackgroundBorderless"
				android:id="@+id/mwBlockTab8"
				android:layout_width="55dp"
				android:layout_height="55dp"
				android:src="@drawable/magicworld_block_grass"
				android:background="@drawable/magicworld_block_grass"/>
			<ImageView
				android:clickable="true"
				android:foreground="?android:attr/selectableItemBackgroundBorderless"
				android:id="@+id/mwBlockTab9"
				android:layout_width="55dp"
				android:layout_height="55dp"
				android:src="@drawable/magicworld_block_grass"
				android:background="@drawable/magicworld_block_grass"/>
			<ImageView
				android:clickable="true"
				android:foreground="?android:attr/selectableItemBackgroundBorderless"
				android:id="@+id/mwBlockTab10"
				android:layout_width="55dp"
				android:layout_height="55dp"
				android:src="@drawable/magicworld_block_grass"
				android:background="@drawable/magicworld_block_grass"/>
		</LinearLayout>
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:alpha="0.7"
			android:gravity="center_horizontal|center_vertical">
			<ImageView
				android:clickable="true"
				android:foreground="?android:attr/selectableItemBackgroundBorderless"
				android:id="@+id/mwBlockTab11"
				android:layout_width="55dp"
				android:layout_height="55dp"
				android:src="@drawable/magicworld_block_grass"
				android:background="@drawable/magicworld_block_grass"/>
			<ImageView
				android:clickable="true"
				android:foreground="?android:attr/selectableItemBackgroundBorderless"
				android:id="@+id/mwBlockTab12"
				android:layout_width="55dp"
				android:layout_height="55dp"
				android:src="@drawable/magicworld_block_grass"
				android:background="@drawable/magicworld_block_grass"/>
			<ImageView
				android:clickable="true"
				android:foreground="?android:attr/selectableItemBackgroundBorderless"
				android:id="@+id/mwBlockTab13"
				android:layout_width="55dp"
				android:layout_height="55dp"
				android:src="@drawable/magicworld_block_grass"
				android:background="@drawable/magicworld_block_grass"/>
			<ImageView
				android:clickable="true"
				android:foreground="?android:attr/selectableItemBackgroundBorderless"
				android:id="@+id/mwBlockTab14"
				android:layout_width="55dp"
				android:layout_height="55dp"
				android:src="@drawable/magicworld_block_grass"
				android:background="@drawable/magicworld_block_grass"/>
			<ImageView
				android:clickable="true"
				android:foreground="?android:attr/selectableItemBackgroundBorderless"
				android:id="@+id/mwBlockTab15"
				android:layout_width="55dp"
				android:layout_height="55dp"
				android:src="@drawable/magicworld_block_grass"
				android:background="@drawable/magicworld_block_grass"/>
		</LinearLayout>
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:alpha="0.7"
			android:gravity="center_horizontal|center_vertical">
			<ImageView
				android:clickable="true"
				android:foreground="?android:attr/selectableItemBackgroundBorderless"
				android:id="@+id/mwBlockTab16"
				android:layout_width="55dp"
				android:layout_height="55dp"
				android:src="@drawable/magicworld_block_grass"
				android:background="@drawable/magicworld_block_grass"/>
			<ImageView
				android:clickable="true"
				android:foreground="?android:attr/selectableItemBackgroundBorderless"
				android:id="@+id/mwBlockTab17"
				android:layout_width="55dp"
				android:layout_height="55dp"
				android:src="@drawable/magicworld_block_grass"
				android:background="@drawable/magicworld_block_grass"/>
			<ImageView
				android:clickable="true"
				android:foreground="?android:attr/selectableItemBackgroundBorderless"
				android:id="@+id/mwBlockTab18"
				android:layout_width="55dp"
				android:layout_height="55dp"
				android:src="@drawable/magicworld_block_grass"
				android:background="@drawable/magicworld_block_grass"/>
			<ImageView
				android:clickable="true"
				android:foreground="?android:attr/selectableItemBackgroundBorderless"
				android:id="@+id/mwBlockTab19"
				android:layout_width="55dp"
				android:layout_height="55dp"
				android:src="@drawable/magicworld_block_grass"
				android:background="@drawable/magicworld_block_grass"/>
			<ImageView
				android:clickable="true"
				android:foreground="?android:attr/selectableItemBackgroundBorderless"
				android:id="@+id/mwBlockTab20"
				android:layout_width="55dp"
				android:layout_height="55dp"
				android:src="@drawable/magicworld_block_grass"
				android:background="@drawable/magicworld_block_grass"/>
		</LinearLayout>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/mwItemBoxLinear"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal|center_vertical"
		android:background="@drawable/mw_anybox_layout"
		android:layout_margin="24dp"
		android:padding="8dp">
	    <LinearLayout
		android:id="@+id/mwItemTab1"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="@drawable/mw_selectedbox_layout"
		android:layout_margin="8dp"
		android:padding="3dp">
		<ImageView
		android:layout_width="44dp"
		android:layout_height="44dp"
		android:src="@drawable/magicworld_item_0"
		android:padding="8dp"/>
	    </LinearLayout>
		<LinearLayout
		android:id="@+id/mwItemTab2"
		android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
		android:background="@drawable/mw_anybox_layout"
		android:layout_margin="8dp"
		android:padding="3dp">
		<ImageView
		android:layout_width="44dp"
		android:layout_height="44dp"
		android:src="@drawable/magicworld_item_0"
		android:padding="8dp"/>
	    </LinearLayout>
		<LinearLayout
			android:id="@+id/mwItemTab3"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:background="@drawable/mw_anybox_layout"
			android:layout_margin="8dp"
			android:padding="3dp">
			<ImageView
				android:layout_width="44dp"
				android:layout_height="44dp"
				android:src="@drawable/magicworld_item_0"
				android:padding="8dp"/>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
